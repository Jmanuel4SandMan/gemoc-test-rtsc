transformation doplatform__resource_org_muml_xsrtsc_xsrtsc_model_XSRTSC_ecore_To_CCSL(in inM : inMM1, out ccsl : TimeModel, out feedback : FeedBack);
property theFeedBackSpec		:FeedBack::ActionModel = null;
